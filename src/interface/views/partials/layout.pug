block variables
    - var title = client.user.username + ' - Dashboard'

html
    head
        title=title
        link(rel="stylesheet" href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Montserrat:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i")
        link(rel="stylesheet" href="/css/styles.css")
        link(rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css")
        link(rel="stylesheet" href="//cdn.jsdelivr.net/npm/@sweetalert2/theme-dark@4/dark.css")
        link(rel="icon" href=client.user.displayAvatarURL({dynamic: true}) type="image/png")
        script(src="https://unpkg.com/@popperjs/core@2")
        script(src="https://unpkg.com/tippy.js@6")
        script(src="//cdn.jsdelivr.net/npm/sweetalert2@11/dist/sweetalert2.min.js")
        block stylesheets
    body
        main.container
            block content

        a.logout-button(href=baseUrl + "logout") Log out

        block scripts
