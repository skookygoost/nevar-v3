{
  "general": {
    "description": "Zeigt Informationen zum Discord-Server ℹ️",
    "usage": "|- serverinfo",
    "example": "|- serverinfo » Zeigt allgemeine Informationen über den Discord-Server",
    "emoji": "ℹ️"
  },

  "main": {
    "fields": {
      "name": {
        "name": "{emotes.pencil} Name",
        "value": "```{guildName}```"
      },
      "id": {
        "name": "{emotes.id} ID",
        "value": "```{guildId}```"
      },
      "owner": {
        "name": "{emotes.crown} Besitzer",
        "value": "```{guildOwner}```"
      },
      "members": {
        "name": "{emotes.people} Mitglieder",
        "value": "```{memberCount}```"
      },
      "channels": {
        "name": "{emotes.channel} Channel",
        "value": "```{channelCount} ({text} Text, {voice} Sprach)```"
      },
      "roles": {
        "name": "{emotes.roles} Rollen",
        "value": "```{roleCount}```"
      },
      "createdAt": {
        "name": "{emotes.calendar} Erstellt am",
        "value": "```{createdAt}```"
      },
      "createdBefore": {
        "name": "{emotes.calendar} Erstellt vor",
        "value": "```{createdBefore}```"
      },
      "boosts": {
        "name": "{emotes.boost} Boosts",
        "value": "```{boosts}```"
      },
      "boostLevel": {
        "name": "{emotes.boost} Boost Level",
        "value": "```{boostLevel}```"
      },
      "boostsUntil": {
        "name": "{emotes.boost} Boosts bis Level {level}",
        "value": "```{boostsUntil}```"
      }
    }
  }
}
